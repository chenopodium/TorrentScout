<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mode PUBLIC
          "-//NetBeans//DTD Mode Properties 2.0//EN"
          "http://www.netbeans.org/dtds/mode-properties2_0.dtd">

<mode version="2.0">
    <name unique="ionogram_mode" />
    <kind type="view" />
    <state type="joined" />
    <constraints>
        <path orientation="vertical" number="50" weight="0.1" />
        <path orientation="horizontal" number="20" weight="1.0" />
    </constraints>    
    <empty-behavior permanent="true" />
</mode>
